<Window x:Class="KeyboardTrainer.Windows.TrainerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:KeyboardTrainer.Windows"
        mc:Ignorable="d"
        Title="TrainerWindow" Height="350" Width="650"
        Closed="CloseTrainer"
        KeyDown="EnterChar">
    <Grid>
        <Label x:Name="Info" FontSize="20" HorizontalAlignment="Left" VerticalAlignment="Top">
            Время:
        </Label>
        <Label x:Name="ExtendedInfo" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Top">
            Уровень:
        </Label>
        <Label x:Name="DuringAnswer" FontSize="20" Margin="0,160,0,0"></Label>
    </Grid>
</Window>
